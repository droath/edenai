{
    "name": "droath/edenai",
    "description": "This is my package edenai",
    "keywords": [
        "Droath",
        "edenai"
    ],
    "homepage": "https://github.com/droath/edenai",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Tomka",
            "email": "underoathosx@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "php-http/discovery": "^1.20",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required for making HTTP requests if no other PSR-18 client is provided"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.10",
        "laravel/pint": "^1.14",
        "mockery/mockery": "^1.6",
        "nyholm/psr7": "^1.8",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "php-http/mock-client": "^1.6",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Droath\\Edenai\\": "src/",
            "Droath\\Edenai\\DTOs\\": "src/DTOs/",
            "Droath\\Edenai\\Exceptions\\": "src/Exceptions/",
            "Droath\\Edenai\\Http\\": "src/Http/",
            "Droath\\Edenai\\Middleware\\": "src/Middleware/",
            "Droath\\Edenai\\Resources\\": "src/Resources/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Droath\\Edenai\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
